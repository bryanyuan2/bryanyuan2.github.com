{
  "name": "bryanyuan2.github.io",
  "version": "1.0.0",
  "description": "bryanyuan2 Curriculum Vitae",
  "main": "index.html",
  "scripts": {
    "clean": "rimraf ./js/build/*.js",
    "eslint": "eslint './js/app/**/*.tsx' './test/app/**/*.tsx'",
    "eslint-fix": "eslint './js/app/**/*.tsx' './test/app/**/*.tsx' --fix",
    "jsonlint:data": "jsonlint-cli './asserts/data/*.json'",
    "jsonlint:mockdata": "jsonlint-cli './test/app/mock/data/*.json'",
    "test": "npm run jsonlint:mockdata && jest",
    "test-cov": "jest --coverage",
    "build:css": "sass ./asserts/css/src/app.scss ./asserts/css/min/app.min.css --style compressed",
    "connect": "live-server --port=3000 --watch=./ --no-browser",
    "watch:dev": "nodemon --watch './index.html' --watch './asserts/css/src/*.scss' --watch './js/app/**/*.js' --exec \"npm run build:dev:js && npm run build:css\"",
    "build:dev:ts": "NODE_ENV=development browserify ./js/app/app.tsx -t [ babelify --extensions .ts,.tsx ] -o ./js/build/bundle.js",
    "build:prod:ts": "NODE_ENV=production browserify ./js/app/app.tsx -t [ babelify --extensions .ts,.tsx ] | uglifyjs -c -m -o ./js/build/bundle.js",
    "start:dev": "npm run jsonlint:data && npm run build:css && npm run build:dev:ts && npm run connect && npm run watch:dev",
    "start:prod": "npm run jsonlint:data && npm run build:css && npm run build:prod:ts && npm run connect"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bryanyuan2/bryanyuan2.github.com"
  },
  "author": {
    "name": "Chun-Yuan (Bryan) Cheng",
    "email": "bryanyuan2@gmail.com",
    "url": "https://bryanyuan2.github.io"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/eslint-parser": "^7.28.0",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.8",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^10.0.0",
    "babel-preset-react": "^6.24.1",
    "babelify": "^10.0.0",
    "browserify": "^16.5.2",
    "chai": "^4.2.0",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.1",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^26.6.3",
    "jsonlint-cli": "^1.0.1",
    "live-server": "^1.2.2",
    "rimraf": "^6.0.1",
    "sass": "^1.90.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "typescript-eslint-parser": "^21.0.2",
    "uglifyjs": "^2.4.11"
  },
  "dependencies": {
    "envify": "^4.1.0",
    "jquery": "^3.7.1",
    "lightbox2": "^2.11.5",
    "lodash": "^4.17.15",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0"
  }
}
